	keywords = {Psych difference},
	file = {SAGE PDF Full Text:/Users/ellenying/Zotero/storage/52MIGA49/Finkel and Eastwick - 2009 - Arbitrary Social Norms Influence Sex Differences i.pdf:application/pdf}
}

