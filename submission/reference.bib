
@article{regan_sex_2006,
	title = {Sex differences and similarities in frequency and intensity of sexual desire},
	volume = {34},
	doi = {10.2224/sbp.2006.34.1.95},
	abstract = {Only within the past decade have social scientists commonly recognized the phenomenon of sexual desire as a distinct and vital component of human sexual response. Of the various factors believed to be associated with sexual desire, gender (biological sex) is presumed by many theorists
to be one of the most important. Limited empirical work suggests that men experience desire more frequently than do women; however, sex differences in intensity or level of desire have yet to be examined. This study explored both the self-reported frequency and intensity of sexual desire among
an ethnically diverse sample of 676 men and women. As hypothesized, men reported experiencing a higher overall level of sexual desire than did women. Sex differences also were found with respect to frequency of sexual desire. Men reported experiencing sexual desire more often than did women
and, when asked to estimate the actual frequency with which they experienced desire, men's estimated frequency (37 times per week) was significantly higher than women's (9 times per week). These results do not imply that men always feel desire or that women lack sexual desire. In
fact, virtually every participant in this study reported feeling sexual desire on a regular basis. This suggests that desire may be the most universal sexual response experienced by both men and women.},
	pages = {95--102},
	number = {1},
	journaltitle = {Social Behavior and Personality: an international journal},
	shortjournal = {Social Behavior and Personality: an international journal},
	author = {Regan, Pamela C. and Atkins, Leah},
	date = {2006-01-01},
	keywords = {Psych difference},
	file = {Regan and Atkins - 2006 - Sex differences and similarities in frequency and .pdf:/Users/ellenying/Zotero/storage/2WPLIZ9I/Regan and Atkins - 2006 - Sex differences and similarities in frequency and .pdf:application/pdf}
}

@article{schmitt_universal_2003,
	title = {Universal sex differences in the desire for sexual variety: Tests from 52 nations, 6 continents, and 13 islands.},
	volume = {85},
	issn = {1939-1315},
	url = {https://psycnet.apa.org/fulltext/2003-05568-009.html},
	doi = {10.1037/0022-3514.85.1.85},
	shorttitle = {Universal sex differences in the desire for sexual variety},
	abstract = {Evolutionary psychologists have hypothesized that men and women possess both long-term and shortterm mating strategies, with men’s short-term strategy differentially rooted in the desire for sexual variety. In this article, findings from a cross-cultural survey of 16,288 people across 10 major world regions (including North America, South America, Western Europe, Eastern Europe, Southern Europe, Middle East, Africa, Oceania, South/Southeast Asia, and East Asia) demonstrate that sex differences in the desire for sexual variety are culturally universal throughout these world regions. Sex differences were evident regardless of whether mean, median, distributional, or categorical indexes of sexual differentiation were evaluated. Sex differences were evident regardless of the measures used to evaluate them. Among contemporary theories of human mating, pluralistic approaches that hypothesize sex differences in the evolved design of short-term mating provide the most compelling account of these robust empirical findings.},
	pages = {85},
	number = {1},
	journaltitle = {Journal of Personality and Social Psychology},
	author = {Schmitt, David P.},
	urldate = {2021-12-26},
	date = {2003},
	langid = {english},
	note = {Publisher: {US}: American Psychological Association},
	keywords = {To read, Psych difference},
	file = {Full Text:/Users/ellenying/Zotero/storage/LLBGM3WX/Schmitt - Universal sex differences in the desire for sexual.pdf:application/pdf;Snapshot:/Users/ellenying/Zotero/storage/8JUWJITU/2003-05568-009.html:text/html}
}

@article{buss_sexual_1993,
	title = {Sexual strategies theory: An evolutionary perspective on human mating},
	volume = {100},
	url = {https://psycnet.apa.org/fulltext/1993-29295-001.html},
	doi = {doi:10.1037/0033-295X.100.2.204d},
	abstract = {This article proposes a contextual-evolutionary theory of human mating strategies. Both men and women are hypothesized to have evolved distinct psychological mechanisms that underlie short-term and long-term strategies. Men and women confront different adaptive problems in short-term as opposed to long-term mating contexts. Consequently, different mate preferences become activated from their strategic repertoires. Nine key hypotheses and 22 predictions from Sexual Strategies Theory are outlined and tested empirically. Adaptive problems sensitive to context include sexual accessibility, fertility assessment, commitment seeking and avoidance, immediate and enduring resource procurement, paternity certainty, assessment of mate value, and parental investment. Discussion summarizes 6 additional sources of behavioral data, outlines adaptive problems common to both sexes, and suggests additional contexts likely to cause shifts in mating strategy.},
	pages = {204--232},
	number = {2},
	journaltitle = {Psychological Review},
	author = {Buss, David M. and Schmitt, David P.},
	urldate = {2021-12-26},
	date = {1993},
	keywords = {To read, Mating strategy, Psych difference},
	file = {Buss and Schmitt - 1993 - Sexual strategies theory An evolutionary perspect.pdf:/Users/ellenying/Zotero/storage/SDBNIAES/Buss and Schmitt - 1993 - Sexual strategies theory An evolutionary perspect.pdf:application/pdf;Sexual Strategies Theory\: An evolutionary perspective on human mating.:/Users/ellenying/Zotero/storage/JGGA7BQS/1993-29295-001.html:text/html}
}

