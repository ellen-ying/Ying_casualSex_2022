	langid = {english},
	file = {Springer Full Text PDF:/Users/ellenying/Zotero/storage/P8X2XQQV/Shackelford et al. - 2004 - Sex Differences in Sexual Psychology Produce Sex-S.pdf:application/pdf}
}

@incollection{conroy-beam_human_2020,
