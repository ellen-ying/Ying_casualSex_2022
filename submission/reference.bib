}

@article{schmitt_are_2001,
	title = {Are men really more 'oriented' toward short-term mating than women? A critical review of theory and research},
	volume = {3},
	issn = {1461-6661},
	url = {https://doi.org/10.1080/14616660110119331},
	doi = {10.1080/14616660110119331},
	shorttitle = {Are men really more 'oriented' toward short-term mating than women?},
	abstract = {According to Sexual Strategies Theory (D.M. Buss and D.P.Schmitt 1993), both men and women possess psychological adaptations for short-term mating. However, men may possess three adaptations that make it seem as though they are generally more 'oriented' toward short-term mating than women: (1) Men possess greater desire for short-term sexual relationships than women; (2) Men prefer larger numbers of sexual partners over time than women; and (3) Men require less time before consenting to sex than women. We review a wide body of psychological theory and evidence that corroborates the presence of these adaptations in men's short-term sexual psychology. We also correct some recurring misinterpretations of Sexual Strategies Theory, such as the mistaken notion that women are designed solely for long-term mating. Finally, we document how the observed sex differences in short-term mating complement some feminist theories and refute competing evolutionary theories of human sexuality.},
	pages = {211--239},
	number = {3},
	journaltitle = {Psychology, Evolution \& Gender},
	author = {Schmitt, David P. and Shackelford, Todd K. and Buss, David M.},
	urldate = {2022-03-02},
	date = {2001-01-01},
	note = {Publisher: Routledge
\_eprint: https://doi.org/10.1080/14616660110119331},
	keywords = {Differences, Human Mating Strategies},
	file = {Schmitt et al. - 2001 - Are men really more 'oriented' toward short-term m.pdf:/Users/ellenying/Zotero/storage/TBPP4VT8/Schmitt et al. - 2001 - Are men really more 'oriented' toward short-term m.pdf:application/pdf;Snapshot:/Users/ellenying/Zotero/storage/6V2FPSJL/14616660110119331.html:text/html}
}

@article{schmitt_reexamination_2012,
