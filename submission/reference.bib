	langid = {english},
	note = {Number: 4
Publisher: Nature Publishing Group},
	keywords = {Mobility, Biological anthropology, Social anthropology},
	file = {Full Text PDF:/Users/ellenying/Zotero/storage/RMKIG2H2/Wood et al. - 2021 - Gendered movement ecology and landscape use in Had.pdf:application/pdf;Snapshot:/Users/ellenying/Zotero/storage/7RDS8JE9/s41562-020-01002-7.html:text/html}
